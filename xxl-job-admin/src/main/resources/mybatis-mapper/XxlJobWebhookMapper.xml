<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xxl.job.admin.dao.XxlJobWebhookDao">

	<resultMap id="XxlJobWebhook" type="com.xxl.job.admin.core.model.XxlJobWebhook" >
		<result column="id" property="id" />
		<result column="webhook_url" property="webhookUrl" />
	    <result column="webhook_type" property="webhookType" />
	    <result column="ctime" property="ctime" />
	    <result column="mtime" property="mtime" />
	</resultMap>

	<select id="findByWebhookType" parameterType="java.util.HashMap" resultMap="XxlJobWebhook">
		SELECT * FROM xxl_job_webhook AS t
		WHERE t.webhook_type = #{webhookType}
	</select>

</mapper>